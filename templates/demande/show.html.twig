{% extends 'inside.html.twig' %}

{% block title %}Demande{% endblock %}

{% block main %}
    <h1>Demande</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ demande.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ demande.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ demande.description }}</td>
            </tr>
            <tr>
                <th>Date_insertion</th>
                <td>{{ demande.dateInsertion ? demande.dateInsertion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_demande_index') }}">back to list</a>

    <a href="{{ path('app_demande_edit', {'id': demande.id}) }}">edit</a>

    {{ include('demande/_delete_form.html.twig') }}
    {{ render(controller('App\\Controller\\HistoriqueController::elementHistorique', {
        'type': demande,
    'id': demande.id,
    'limit': 5
})) }}
{% endblock %}
